    "axios": "^0.21.1",
    "bluebird": "^3.7.2",
    "bwip-js": "^3.0.4",
    "compression": "^1.7.4",
    "dayjs": "^1.11.5",
    "dotenv": "^10.0.0",
    "faker": "5.5.3",
    "gravatar": "^1.8.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "moleculer": "^0.14.16",
    "moleculer-auto-openapi": "^1.0.7",
    "moleculer-db": "^0.8.15",
    "moleculer-db-adapter-mongo": "^0.4.13",
    "moleculer-db-adapter-sequelize": "^0.2.11",
