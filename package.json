{
  "name": "api",
  "version": "1.0.0",
  "description": "api",
  "scripts": {
    "devD": "moleculer-runner --repl --hot --config moleculer.dev.config.js services/**/services/*.service.js",
    "startD": "moleculer-runner --config moleculer.config.js services/**/services/*.service.js",
    "dev": "nodemon ./src/services/**/*.service.ts",
    "devIN": "nodemon ./src/services/$SERVICE/$SERVICE.service.ts",
    "start": "moleculer-runner --config ./dist/moleculer.dev.config.js ./dist/src/services/$SERVICE/$SERVICE.service.js",
    "ci": "jest --watch",
    "test": "jest --coverage --config ./jest.config.ts",
    "build": "rm -rf ./dist && tsc -p tsconfig.json",
    "lint": "eslint . --ext .js,.jsx --fix",
    "format": "prettier --ignore-path .gitignore -w .",
    "format:check": "prettier --ignore-path .gitignore --check .",
    "prepare": "husky install",
    "prettier-c": "prettier --ignore-path .gitignore --check .",
    "cli": "moleculer connect TCP"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "author": "rfcku",
  "devDependencies": {
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@typegoose/typegoose": "^10.5.0",
    "@types/compression": "^1.7.5",
    "@types/faker": "^6.6.9",
    "@types/gravatar": "^1.8.6",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^18.19.33",
    "@types/oauth2-server": "^3.0.16",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^3.4.1",
    "husky": "^7.0.4",
    "jest": "^29.7.0",
    "jest-cli": "^27.5.1",
    "moleculer-repl": "^0.6.6",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "superagent": "^6.1.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "type-fest": "^3.13.1",
    "typescript": "^4.9.5",
    "zod": "^3.23.8"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "aws-sdk": "^2.1628.0",
    "axios": "^0.21.4",
    "bluebird": "^3.7.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.11",
    "dotenv": "^10.0.0",
    "gravatar": "^1.8.2",
    "jsonwebtoken": ">=9.0.0",
    "md5": "^2.3.0",
    "mime": "^3.0.0",
    "moleculer": "^0.14.33",
    "moleculer-auto-openapi": "^1.1.6",
    "moleculer-db": "^0.8.25",
    "moleculer-db-adapter-mongo": "^0.4.19",
    "moleculer-io": "^2.2.0",
    "moleculer-mail": "^1.2.6",
    "moleculer-prometheus": "^0.2.5",
    "moleculer-web": "^0.10.7",
    "moment-timezone": "^0.5.45",
    "mongodb": "^4.17.2",
    "mongoose": "^8.4.0",
    "node-fetch": "^2.7.0",
    "node-object-hash": "^3.0.0",
    "node-res": "^5.0.1",
    "oauth2-server": "^3.1.1",
    "sha256": "^0.2.0",
    "socket.io": "^4.7.5",
    "stream": "^0.0.2",
    "util": "^0.12.5"
  },
  "eslintIgnore": [
    "*.test.ts"
  ],
  "license": "MIT",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint",
      "pre-push": "yarn run lint && yarn run test"
    }
  },
  "email": "rfcku@email.com",
  "jest": {
    "coverageDirectory": "<rootDir>/coverage",
    "testEnvironment": "node",
    "rootDir": "./",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "roots": [
      "test"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/*.spec.(ts|js)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "collectCoverageFrom": [
      "**/*.(ts|js)"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "setupFilesAfterEnv": [
      "dotenv/config"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover"
    ],
    "verbose": true
  },
  "main": "main.js"
}
