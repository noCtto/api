{
  "name": "api-gateway",
  "version": "1.0.0",
  "description": "api",
  "scripts": {
    "devD": "moleculer-runner --repl --hot --config moleculer.dev.config.js services/**/services/*.service.js",
    "startD": "moleculer-runner --config moleculer.config.js services/**/services/*.service.js",
    "dev": "nodemon ./src/services/**/*.service.ts",
    "start": "moleculer-runner --config ./dist/moleculer.dev.config.js ./dist/services/*.service.js",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint . --ext .js,.jsx --fix",
    "format": "prettier --ignore-path .gitignore -w .",
    "format:check": "prettier --ignore-path .gitignore --check .",
    "prepare": "husky install",
    "prettier-c": "prettier --ignore-path .gitignore --check .",
    "cli": "moleculer connect TCP"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "author": "rfcku",
  "devDependencies": {
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.22.5",
    "@typegoose/typegoose": "^10.3.0",
    "@types/compression": "^1.7.2",
    "@types/faker": "^6.6.9",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.14.6",
    "@types/oauth2-server": "^3.0.13",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^3.4.1",
    "husky": "^7.0.2",
    "jest": "^29.5.0",
    "jest-cli": "^27.1.0",
    "moleculer-repl": "^0.6.6",
    "nodemon": "^2.0.21",
    "prettier": "^2.3.2",
    "superagent": "^6.1.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "type-fest": "^3.13.0",
    "typescript": "^4.9.5",
    "zod": "^3.21.4"
  },
  "dependencies": {
    "@faker-js/faker": "^7.6.0",
    "@sendgrid/mail": "^7.7.0",
    "@types/gravatar": "^1.8.3",
    "aws-sdk": "^2.976.0",
    "axios": "^0.21.1",
    "bluebird": "^3.7.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.5",
    "dotenv": "^10.0.0",
    "gravatar": "^1.8.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "md5": "^2.3.0",
    "mime": "^3.0.0",
    "moleculer": "^0.14.21",
    "moleculer-auto-openapi": "^1.0.7",
    "moleculer-db": "^0.8.15",
    "moleculer-db-adapter-mongo": "^0.4.17",
    "moleculer-db-adapter-sequelize": "^0.2.11",
    "moleculer-io": "^2.1.0",
    "moleculer-mail": "^1.2.5",
    "moleculer-prometheus": "^0.2.5",
    "moleculer-web": "^0.10.5",
    "moment-timezone": "^0.5.33",
    "mongodb": "^4.1.3",
    "mongoose": "^6.0.0",
    "node-fetch": "^2.6.1",
    "node-res": "^5.0.1",
    "oauth2-server": "^3.1.1",
    "sha256": "^0.2.0",
    "socket.io": "^4.5.4",
    "stream": "^0.0.2",
    "util": "^0.12.4"
  },
  "eslintIgnore": [
    "*.test.js"
  ],
  "license": "MIT",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint",
      "pre-push": "yarn run lint && yarn run test"
    }
  },
  "email": "rfcku@email.com",
  "jest": {
    "coverageDirectory": "<rootDir>/coverage",
    "testEnvironment": "node",
    "rootDir": "./",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "roots": [
      "test"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/*.spec.(ts|js)"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "collectCoverageFrom": [
      "**/*.(ts|js)"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "setupFilesAfterEnv": [
      "dotenv/config"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/services/$1",
      "^@config/(.*)$": "<rootDir>/src/services/$1",
      "^@accounts/(.*)$": "<rootDir>/src/services/$1",
      "^@lib/(.*)$": "<rootDir>/src/lib/$1",
      "^@communities/(.*)$": "<rootDir>/src/services/communities/$1",
      "^@comments/(.*)$": "<rootDir>/src/services/comments/$1",
      "^@threads/(.*)$": "<rootDir>/src/services/threads/$1",
      "^@posts/(.*)$": "<rootDir>/src/services/posts/$1",
      "^@votes/(.*)$": "<rootDir>/src/services/votes/$1",
      "^@mixins/(.*)$": "<rootDir>/src/mixins/$1",
      "^@utils/(.*)$": "<rootDir>/src/utils/$1"
    },
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover"
    ],
    "verbose": true
  },
  "main": "main.js"
}
