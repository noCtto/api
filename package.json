{
  "name": "api-gateway",
  "version": "1.0.0",
  "description": "API Gateway - microservices with moleculerjs ðŸ’ª",
  "scripts": {
    "dev": "moleculer-runner --repl --hot --config moleculer.dev.config.js services/**/services/*.service.js",
    "start": "moleculer-runner --config moleculer.config.js services/**/services/*.service.js",
    "ci": "jest --watch",
    "test": "jest --coverage",
    "test:ci": "echo 'All test passed ðŸ’ª'",
    "build": "echo 'No build!? ... ok'",
    "lint": "eslint . --ext .js,.jsx --fix",
    "format": "prettier --ignore-path .gitignore -w .",
    "format:check": "prettier --ignore-path .gitignore --check .",
    "prepare": "husky install",
    "prettier-c": "prettier --ignore-path .gitignore --check .",
    "cli": "moleculer connect TCP",
    "clean": "sudo rm -rf node_modules && rm -rf yarn.lock && rm -rf package-lock.json && rm -rf coverage && rm -rf .husky",
    "test:unit": "jest \"unit/.*\\.test\\.js\" --coverage",
    "test:int": "jest \"integral/.*\\.test\\.js\" --coverage"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "engines": {
    "node": "16.x"
  },
  "author": "Antonio Orozco <orozco6453@gmail.com>",
  "devDependencies": {
    "@babel/preset-env": "^7.15.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^3.4.1",
    "husky": "^7.0.2",
    "jest": "^27.2.4",
    "jest-cli": "^27.1.0",
    "moleculer-repl": "^0.6.6",
    "prettier": "^2.3.2"
  },
