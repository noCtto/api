  "eslintIgnore": [
    "*.test.js"
  ],
  "license": "MIT",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint",
      "pre-push": "yarn run lint && yarn run test"
    }
  },
  "email": "rfcku@email.com",
  "jest": {
    "coverageDirectory": "../coverage",
